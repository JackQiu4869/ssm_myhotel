<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.bdqn.dao.SuperAdministratorMapper">

    <!--根据登录名查询-->
    <select id="findAdminByLoginName" resultType="com.bdqn.entity.SuperAdministrator">
        select * from administrators where name = #{loginName}
    </select>

</mapper>